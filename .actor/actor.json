{
    "actorSpecification": 1,
    "name": "Apify-RSS-Search",
    "title": "Apify RSS Search",
    "description": "Apify actor to perform RSS searches.",
    "version": "0.1",
    "buildTag": "latest",
    "meta": {
        "templateId": "python-empty"
    },
    "dockerfile": "./Dockerfile",
    "storages": {
        "dataset": {
            "actorSpecification": 1,
            "views": {
                "overview": {
                    "title": "Overview",
                    "transformation": {
                        "fields": [
                            "title",
                            "link",
                            "text",
                            "title_score",
                            "description_score",
                            "score"
                        ]
                    },
                    "display": {
                        "component": "table",
                        "properties": {
                            "title": {
                                "label": "Title",
                                "format": "text"
                            },
                            "link": {
                                "label": "Link",
                                "format": "text"
                            },
                            "text": {
                                "label": "Text",
                                "format": "text"
                            },
                            "score": {
                                "label": "Score",
                                "format": "number"
                            },
                            "title_score": {
                                "label": "Title Score",
                                "format": "number"
                            },
                            "description_score": {
                                "label": "Description Score",
                                "format": "number"
                            }
                        }
                    }
                }
            }
        }
    }
}
